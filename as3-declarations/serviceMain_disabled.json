{
   "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.0.0",
        "id": "urn:uuid:33045210-3ab8-4636-9b2a-c98d22ab915d",
        "label": "Sample 1",
        "remark": "Simple HTTP Service with Round-Robin Load Balancing",
        "Tom": {
            "class": "Tenant",
            "A1": {
                "class": "Application",
                "template": "http",
                "serviceMain": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.1.99"
                    ],
                    "enable": false,
                    "pool": "tom_pool"
                },
                "http80": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.1.80"
                    ],
                    "enable": true,
                    "pool": "tom_pool"
                },
                "tom_pool": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "10.1.1.10",
                                "10.1.1.11"
                            ]
                        }
                    ]
                }
            },
            "A2": {
                "class": "Application",
                "template": "http",
                "serviceMain": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.1.199"
                    ],
                    "enable": false,
                    "pool": "tom_pool2"
                },
                "http81": {
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "10.1.1.81"
                    ],
                    "pool": "tom_pool2"
                },
                "tom_pool2": {
                    "class": "Pool",
                    "monitors": [
                        "http"
                    ],
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": [
                                "10.1.1.12",
                                "10.1.1.13"
                            ]
                        }
                    ]
                }
            }
        }
    }
}